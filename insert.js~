var mongoose = require('mongoose');
mongoose.connect('mongodb://localhost/test');

var ViewQuery = mongoose.model('ViewQuery', { body: String });

var prof_viewquery = ("prefix Prof: <http://prof.test/> " +
   "prefix Lab: <http://lab.test/>  " + 
        "select * " +
        "where " +
        "{ ?profID Prof:Name ?Name ." +
        "?profID Lab:ID ?labID . }");
var lab_viewquery = ("prefix Prof: <http://prof.test/> " +
    "prefix Lab: <http://lab.test/>  " + 
    "select * " +
    "where " +
    "{ ?ID Lab:Name ?Name }");

var p = new ViewQuery({body: prof_viewquery});
var l = new ViewQuery({body: lab_viewquery});

p.save(function (err, p) {
  if (err) return console.error(err);
  console.log('p saved');
});
l.save(function (err, p) {
  if (err) return console.error(err);
  console.log('l saved');
});
